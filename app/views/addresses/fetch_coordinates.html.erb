<h1>Coordinates Lookup</h1>

<form>
  <label for="form1">What address would you like the coordinates for?</label>
  <input type="text" id="form1" name="address">
  <button>Search</button>
</form>




<dl>
  <dt>Address:</dt>
  <dd><%= @address %></dd>

  <dt>URL Safe Address:</dt>
  <dd><%= @url_safe_address %></dd>

  <dt>Latitude:</dt>
  <dd><%= @latitude %></dd>

  <dt>Longitude:</dt>
  <dd><%= @longitude %></dd>
</dl>
